0) cat /etc/passwd

1) sed -n '/^1.0/ p' 2019-10.csv > vendor2.csv

2) sed -e 's/://g' -e 's/\"//g' -e 's/-//g' 2019-10.csv > no-separators.csv

3) sed -e 's/\.[[:digit:]]*//g' no-separators.csv > no-fractions.csv

4) awk -F , '$17>9 {tip[$14]++} END {for (amount in tip) print amount, tip[amount]}' 2019-10.csv > results.txt

5) awk '{tip[$1]=$2} END {asort(tip); for (amount in tip) print amount, tip[amount]}' results.txt | tac | head -5
